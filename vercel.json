{
  "version": 2,
  "buildCommand": "node vercel-build.js && npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "env": {
    "NODE_ENV": "production",
    "VITE_CONFIG_FILE": "vite.config.vercel.js"
  }
}